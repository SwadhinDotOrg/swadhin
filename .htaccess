Options -Indexes
ErrorDocument 404 "<h1 style = 'color:red;'>Error 404:Page Not Found!</h1> <br /> <a href = 'http://onlyfone.com/contact.html#feedback'>Click here to report</a>"
RewriteEngine on
RewriteRule ^([a-zA-Z0-9-_]+)\.bin$ CONTROL/$1.php [QSA,L]
RewriteRule ^([a-zA-Z0-9-_/]+)\.html$ index.php?page=$1 [QSA,L]
RewriteRule ^feed/?$ CONTROL/feed_gen.php [QSA,L]
RewriteRule ^Sitemap\.xml$ CONTROL/sm_gen.php [QSA,L]